@ARTICLE{Ahvazi2000,
  author = {Ahvazi, B. and Coulombe, R. and Delarge, M. and Vedadi, M. and Zhang,
	L. and Meighen, E. and Vrielink, A.},
  title = {Crystal structure of the NADP+-dependent aldehyde dehydrogenase from
